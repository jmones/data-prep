{
  "mimeType": "text/csv",
  "extension": ".csv",
  "id": "CSV",
  "name": "CSV",
  "needParameters": true,
  "defaultExport": false,
  "enabled": true,
  "disableReason": "",
  "title": "Export to CSV",
  "parameters": [
    {
      "name": "csvSeparator",
      "type": "select",
      "implicit": false,
      "canBeBlank": true,
      "placeHolder": "",
      "configuration": {
        "values": [
          {
            "value": ";",
            "label": "Semicolon"
          },
          {
            "value": "\t",
            "label": "Tabulation"
          },
          {
            "value": " ",
            "label": "Space"
          },
          {
            "value": ",",
            "label": "Comma"
          },
          {
            "value": "|",
            "label": "Pipe"
          }
        ],
        "multiple": false
      },
      "radio": true,
      "description": "Select character to use as a delimiter",
      "label": "Delimiter",
      "default": ";"
    },
    {
      "name": "fileName",
      "type": "string",
      "implicit": false,
      "canBeBlank": false,
      "placeHolder": "",
      "description": "Name of the generated export file",
      "label": "Filename",
      "default": ""
    }
  ]
}
